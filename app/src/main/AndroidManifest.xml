        android:icon="@mipmap/ic_retretku"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_retretku_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".HomeActivity"
